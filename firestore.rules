rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    // Allow read and write access to all documents for any signed-in user.
    // This is a permissive rule suitable for development, but you should
    // implement more granular rules for production.
    match /{document=**} {
      allow read, write: if request.auth != null;
    }
  }
}
